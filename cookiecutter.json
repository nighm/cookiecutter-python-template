{
    "project_name": "Python Project Template",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
    "package_name": "{{ cookiecutter.project_slug.replace('-', '_') }}",
    "project_description": "A production-ready Python project template with comprehensive code quality tools.",
    "full_name": "Your name",
    "email": "your.email@example.com",
    "github_username": "username",
    "version": "0.1.0",
    "python_version": {
        "min": "3.11.0",
        "max": "3.12.0",
        "recommended": "3.11.8",
        "description": "推荐使用Python 3.11.8，支持3.11-3.12版本范围。"
    },
    "use_pytest": "y",
    "use_black": "y",
    "use_mypy": "y",
    "use_isort": "y",
    "use_pylint": "y",
    "use_ruff": "y",
    "use_bandit": "y",
    "use_safety": "y",
    "use_xenon": "y",
    "use_radon": "y",
    "use_pytype": "n",
    "use_scalene": "n",
    "use_pre_commit": "y",
    "use_mkdocs": "y",
    "include_examples": "y",
    "use_docker": "n",
    "use_make": "y",
    "use_github_actions": "y",
    "quality_level": "standard",
    "license": "MIT",
    "_copy_without_render": [
        ".github/workflows/*.yml",
        "*.html",
        "*.js",
        "*.css"
    ]
} 